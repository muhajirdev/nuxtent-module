{
  "name": "nuxtent",
  "version": "0.2.59",
  "description": "Content Module for Nuxt.js.",
  "main": "dist/module.js",
  "contributors": [
    "Alid Castano (@alidcastano)"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt-community/nuxtent-module.git"
  },
  "keywords": [
    "Nuxt.js",
    "Vue.js",
    "Content",
    "Blog",
    "Posts",
    "Collections",
    "Markdown",
    "Static"
  ],
  "license": "MIT ",
  "bin": {
    "nuxtent": "bin/nuxtent"
  },
  "scripts": {
    "unit": "cross-env NODE_ENV=development karma start test/unit/karma.conf.js",
    "e2e": "cross-env NODE_ENV=test ava --verbose --serial tests/e2e",
    "test": "npm run unit --single-run && npm run e2e",
    "build": "NODE_ENV=production rollup -c rollup.config.js",
    "watch": "npm run build -- -w",
    "bump": "npm run build && npm --no-git-tag-version version patch && npm publish"
  },
  "peerDependencies": {
    "@nuxtjs/axios": "^2.1.0"
  },
  "dependencies": {
    "chalk": "^2.0.1",
    "front-matter": "^2.1.2",
    "js-yaml": "^3.9.0",
    "loader-utils": "^1.1.0",
    "markdown-it": "^8.3.1",
    "markdown-it-anchor": "^4.0.0",
    "moment": "^2.18.1",
    "param-case": "^2.1.1",
    "path-to-regexp": "^1.7.0",
    "uppercamelcase": "^3.0.0"
  },
  "devDependencies": {
    "@nuxtjs/axios": "^3.0.1",
    "ava": "^0.21.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.0",
    "cross-env": "^5.0.1",
    "express": "^4.15.3",
    "jsdom": "^11.1.0",
    "karma": "^1.7.0",
    "karma-mocha": "^1.3.0",
    "karma-rollup-preprocessor": "^4.0.2",
    "karma-sinon-chai": "^1.3.1",
    "mocha": "^3.5.0",
    "nuxt": "^1.0.0-alpha.4",
    "rollup": "^0.45.2",
    "rollup-plugin-babel": "^3.0.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-copy": "^0.2.3",
    "rollup-plugin-filesize": "^1.2.1",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-uglify-es": "0.0.1",
    "rollup-watch": "^4.0.0",
    "sinon": "^3.0.0",
    "sinon-chai": "^2.12.0"
  },
  "bugs": {
    "url": "https://github.com/nuxt-community/nuxtent-module/issues"
  },
  "homepage": "https://github.com/nuxt-community/nuxtent-module#readme",
  "directories": {
    "doc": "docs",
    "example": "examples",
    "lib": "lib",
    "test": "tests"
  },
  "author": "Alid Castano"
}
